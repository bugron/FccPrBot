{
  "bugron/TestPrBot": {
    "repoContribPath": "/blob/staging/CONTRIBUTING.md",
    "closeAllPRsMessage": "Unfortunately, we do not accept any PRs at this time. Please, try later.",
    "forbiddenPRUsers": [
      "*"
    ],
    "ignorePRFromUsers": [
        "greenkeeperio-bot",
        "QuincyLarson",
        "BerkeleyTrue",
        "snyk-bot"
    ],
    "actions": [
      "opened",
      "reopened",
      "synchronize"
    ],
    "rules": {
      "critical": {
        "allowedFileNamePatterns": [
          "title"
        ],
        "forbiddenBaseBranchPatterns": [
          "master"
        ],
        "forbiddenHeadBranchPatterns": [
          "master",
          "staging",
          "backup/master",
          "backup/staging"
        ]
      },
      "allowedHeadBranchPatterns": [
        "bug/",
        "chore/",
        "fix/",
        "feat",
        "feature/",
        "translate/"
      ],
      "forbiddenTitleKeywords": [
        "close",
        "closes",
        "closed",
        "fix",
        "fixes",
        "fixed",
        "resolve",
        "resolves",
        "resolved"
      ],
      "maxCommitCount": 1
    }
  }
}
